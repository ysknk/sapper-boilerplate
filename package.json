{
  "name": "app",
  "description": "app",
  "version": "0.0.1",
  "scripts": {
    "dev": "npm run init && sapper dev",
    "_build": "sapper build --legacy",
    "_export": "sapper export --legacy",
    "export": "npm run export:production",
    "export:develop": "npm run init && cross-env NODE_ENV=develop node sapper.export.mjs",
    "export:preview": "npm run init && cross-env NODE_ENV=preview node sapper.export.mjs",
    "export:production": "npm run init && cross-env NODE_ENV=production node sapper.export.mjs",
    "help": "sapper export --help",
    "generate": "npm run export",
    "start": "node __sapper__/build",
    "validate": "svelte-check --ignore src/node_modules/@sapper",
    "init": "npm run minify:init",
    "minify:init": "cp ./minify_html.js node_modules/sapper/dist/",
    "postinstall": "npm run minify:init"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "express": "^4.18.1",
    "polka": "next",
    "sirv": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.4.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@rollup/plugin-url": "^5.0.0",
    "@tsconfig/svelte": "^1.0.10",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.13",
    "@types/node": "^14.11.1",
    "@types/polka": "^0.5.1",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "eslint": "^8.17.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "nib": "^1.2.0",
    "postcss": "^8.4.14",
    "postcss-load-config": "^4.0.1",
    "pug": "^3.0.2",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sapper": "^0.28.0",
    "sapper-environment": "^1.0.1",
    "stylus": "^0.55.0",
    "svelte": "^3.17.3",
    "svelte-check": "^1.0.46",
    "svelte-image": "^0.2.9",
    "svelte-preprocess": "^4.3.0",
    "tslib": "^2.0.1",
    "typescript": "^4.0.3"
  }
}
